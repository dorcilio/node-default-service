{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "strict": 0,
      "space-in-parens": 0,
      "template-curly-spacing": 0,
      "no-console": "off",
      "indent": ["error", 2]
    }
}
